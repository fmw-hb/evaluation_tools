<?xml version="1.0"?>

<launch>
  <arg name="map_file" default="$(find ipa_map_comparison)/maps/map_full.yaml"/>

  <!-- Run the map server -->
    <node name="map_server_ground_truth" pkg="map_server" type="map_server" args="$(arg map_file)" >
    <remap from="map" to="static_map"/>
    </node>

    <!-- Run the map server -->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find ipa_map_comparison)/maps/map.yaml" >
    <remap from="static_map" to="map"/>
    </node>

<node name="ipa_map_comparison_node" pkg="ipa_map_comparison" type="ipa_map_comparison_node" output="screen"/>
</launch>
